<meta charset="utf-8">
<meta content="ie=edge" http-equiv="x-ua-compatible">
<meta content="width=device-width, initial-scale=1.0, maximum-scale=1" name="viewport">

<title>Together</title>
{# <title>{{ config.seo.title }}</title> #}

{# Preload styles #}
<link rel="preload" href="{% webpackAsset 'style.css' %}" as="style">

{# Preload fonts #}
<link rel="preload" href="/assets/fonts/Graphik-Medium-Web.woff2" as="font" type="font/woff2" crossorigin="crossorigin">
<link rel="preload" href="/assets/fonts/Graphik-Medium-Web.woff" as="font" type="font/woff" crossorigin="crossorigin">
<link rel="preload" href="/assets/fonts/Graphik-Regular-Web.woff2" as="font" type="font/woff2" crossorigin="crossorigin">
<link rel="preload" href="/assets/fonts/Graphik-Regular-Web.woff" as="font" type="font/woff" crossorigin="crossorigin">
<link rel="preload" href="/assets/fonts/RecklessNeue-RegularItalic.woff2" as="font" type="font/woff2" crossorigin="crossorigin">
<link rel="preload" href="/assets/fonts/RecklessNeue-RegularItalic.woff" as="font" type="font/woff" crossorigin="crossorigin">
<link rel="preload" href="/assets/fonts/RecklessNeue-Regular.woff2" as="font" type="font/woff2" crossorigin="crossorigin">
<link rel="preload" href="/assets/fonts/RecklessNeue-Regular.woff" as="font" type="font/woff" crossorigin="crossorigin">

{# Preload scripts #}
<link rel="preload" href="{% webpackAsset 'main.js' %}" as="script">

<meta name="description" content="{{ config.seo.description }}">
<meta name="keywords" content="{{ config.seo.keywords | join(', ') }}"/>

{# Facebook Open Graph #}
<meta property="og:url" content="{{ config.seo.url }}">
<meta property="og:type" content="website">
<meta property="og:title" content="{{ config.seo.title }}">
<meta property="og:description" content="{{ config.seo.description }}">
<meta property="og:site_name" content="{{ config.seo.title }}">
<meta property="og:locale" content="en_US">
<meta property="og:image" content="{{ config.seo.image.url }}">
<meta property="og:image:secure_url" content="{{ config.seo.image.url }}">
<meta property="og:image:alt" content="{{ config.seo.image.alt }}">
<meta property="og:image:width" content="{{ config.seo.image.metadata.dimensions.width }}">
<meta property="og:image:height" content="{{ config.seo.image.metadata.dimensions.height }}">

{# Twitter Card #}
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:url" content="{{ config.seo.url }}">
<meta name="twitter:title" content="{{ config.seo.title }}">
<meta name="twitter:description" content="{{ config.seo.description }}">
<meta name="twitter:image" content="{{ config.seo.image.url }}">
<meta name="twitter:image:alt" content="{{ config.seo.image.alt }}">

{# Favicons #}
<link rel="icon" href="/assets/icons/logo_fav.svg">

{# Link stylesheet #}
<link rel="stylesheet" href="{% webpackAsset 'style.css' %}">

{# Link jquery #}
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

{# If browser-sync stops reloading, try uncommenting the below
{% if env !== 'production' %}
  <script async="" src="/browser-sync/browser-sync-client.js?v=2.26.7"></script>
{% endif %}
#}